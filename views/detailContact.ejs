<div class="container">
    <div class="row mt-4">
        <div class="col">
            
            <h1>Detail Contact</h1>

            <div class="card" style="width: 18rem;">
                <div class="card-body">
                  <h5 class="card-title"> <%= cont.name %> </h5>
                  <h6 class="card-subtitle mb-2 text-muted"><%= cont.phone %></h6>
                  <p class="card-text"><%= cont.email %></p>
                  <a href="../contact" class="card-link">Back to contact</a>
                </div>
            </div>

        </div>
    </div>
</div>